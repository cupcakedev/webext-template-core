{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
	"default_icon": {
      "16": "start.jpg",
      "32": "start.jpg",
      "48": "start.jpg",
      "128": "start.jpg"
	}
  }
}